        mc:Ignorable="d"
        Title="Password" Width="300" Height="70">
    <Grid>
        <PasswordBox x:Name="TxtPassword" Height="23" VerticalAlignment="Top" Margin="10,10,9.8,0" KeyUp="TxtPassword_KeyUp" TabIndex="0" />
    </Grid>
</Window>
